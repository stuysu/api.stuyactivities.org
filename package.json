{
	"name": "api.stuyactivities.org",
	"version": "1.0.0",
	"description": "Backend API for StuyActivities",
	"author": "StuySU",
	"main": "src/index.js",
	"scripts": {
		"start": "node --require esm src/index.js",
		"test": "npx nyc mocha --require esm",
		"dev": "nodemon --require esm src/index.js",
		"migrate": "node --require esm node_modules/.bin/sequelize-cli db:migrate",
		"seed": "node --require esm node_modules/.bin/sequelize-cli db:seed:all",
		"prettier": "npx prettier --write .",
		"build": "npm run migrate",
		"authenticate": "node -r esm src/googleApis/authenticate.js",
		"coverage": "nyc report --require esm --reporter=text-lcov | coveralls"
	},
	"dependencies": {
		"apollo-server-errors": "^2.5.0",
		"apollo-server-express": "^3.8.2",
		"axios": "^0.21.2",
		"bcrypt": "^5.0.1",
		"body-parser": "latest",
		"bottleneck": "^2.19.5",
		"chai": "^4.3.6",
		"cloudinary": "^1.28.1",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"coveralls": "^3.1.0",
		"crypto-random-string": "^3.3.1",
		"dataloader": "^2.0.0",
		"email-validator": "^2.0.4",
		"esm": "^3.2.25",
		"express": "latest",
		"faker": "^5.5.3",
		"google-auth-library": "^7.11.0",
		"googleapis": "^81.0.0",
		"graphql": "^15.5.0",
		"graphql-scalars": "^1.14.1",
		"graphql-upload": "^12.0.0",
		"graphql-validation-complexity": "^0.4.2",
		"honeybadger": "^1.4.1",
		"html-entities": "^2.3.2",
		"ip-range-check": "^0.2.0",
		"jsonwebtoken": "^8.5.1",
		"mailparser": "^3.2.0",
		"markdown-it": "^12.3.2",
		"mocha": "^8.4.0",
		"moment-timezone": "^0.5.34",
		"morgan": "^1.10.0",
		"mysql2": "^2.3.3",
		"node-html-parser": "^4.0.0",
		"nodemailer": "^6.7.2",
		"normalize-url": "^6.0.0",
		"npm-run-all": "^4.1.5",
		"nunjucks": "^3.2.3",
		"nyc": "^15.1.0",
		"open-graph-scraper": "^4.8.2",
		"regenerator-runtime": "^0.13.9",
		"request": "^2.88.2",
		"sanitize-html": "^2.3.3",
		"sequelize": "^6.16.1",
		"sequelize-cli": "^6.2.0",
		"turndown": "^7.0.0",
		"url-join": "^4.0.1"
	},
	"devDependencies": {
		"nodemon": "^2.0.15",
		"prettier": "^2.3.0",
		"sqlite3": "^5.0.2"
	},
	"prettier": {
		"useTabs": true,
		"tabWidth": 4,
		"arrowParens": "avoid",
		"trailingComma": "none",
		"semi": true,
		"singleQuote": true,
		"printWidth": 80,
		"endOfLine": "lf"
	},
	"mocha": {
		"recursive": true
	},
	"engines": {
		"node": ">=16.3.0"
	},
	"engineStrict": true
}
