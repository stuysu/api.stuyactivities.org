{
	"name": "api.stuyactivities.org",
	"version": "1.0.0",
	"description": "Backend API for StuyActivities",
	"author": "StuySU",
	"main": "src/index.js",
	"scripts": {
		"test": "npx nyc mocha src/test/**/*.js",
		"dev": "npx nodemon src",
		"start": "node src/index.js",
		"migrate": "npx sequelize db:migrate",
		"prettier": "npx prettier --write .",
		"heroku-postbuild": "npm install pg pg-native && npm run migrate"
	},
	"dependencies": {
		"apollo-server-express": "^2.15.1",
		"axios": "^0.19.2",
		"bcrypt": "^5.0.0",
		"body-parser": "latest",
		"cloudinary": "^1.22.0",
		"connect-session-sequelize": "^7.0.0",
		"cookie-parser": "^1.4.5",
		"cors": "^2.8.5",
		"crypto-random-string": "^3.2.0",
		"dataloader": "^2.0.0",
		"dotenv": "latest",
		"express": "latest",
		"express-session": "^1.17.1",
		"google-auth-library": "^6.0.4",
		"honeybadger": "^1.4.0",
		"ip-range-check": "^0.2.0",
		"morgan": "^1.10.0",
		"mysql2": "^2.1.0",
		"node-html-parser": "^1.2.20",
		"nodemailer": "^6.4.10",
		"nunjucks": "^3.2.1",
		"sequelize": "^6.3.2",
		"sqlite3": "^5.0.0",
		"url-join": "^4.0.1"
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"mocha": "^8.0.1",
		"nodemon": "^2.0.4",
		"nyc": "^15.1.0",
		"prettier": "^2.0.5",
		"request": "^2.88.2",
		"sequelize-cli": "^6.2.0"
	},
	"prettier": {
		"useTabs": true,
		"tabWidth": 4,
		"arrowParens": "avoid",
		"trailingComma": "none",
		"semi": true,
		"singleQuote": true,
		"printWidth": 80,
		"endOfLine": "lf"
	},
	"mocha": {
		"recursive": true
	},
	"babel": {
		"presets": [
			"@babel/es2015"
		],
		"plugins": [
			"@babel/plugin-proposal-class-properties"
		]
	}
}
